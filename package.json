{
  "name": "mp",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./scripts/start.js",
    "start:server": "node ./src/server",
    "build": "node ./scripts/build.js",
    "test": "jest",
    "precommit:test": "jest --coverage --bail",
    "build:app": "node ./scripts/cordova.js && cd cordova && cordova run android"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizavetaageenko/mp.git"
  },
  "author": "Yelyzaveta Buts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lizavetaageenko/mp/issues"
  },
  "homepage": "https://github.com/lizavetaageenko/mp#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.24.0",
    "detect-port": "^1.0.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^15.1.1",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.9.3",
    "postcss-loader": "^0.13.0",
    "pre-commit": "^1.1.3",
    "resolve-url-loader": "^1.6.0",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.2",
    "sasslint-webpack-plugin": "^1.0.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-cordova-plugin": "^0.1.6",
    "webpack-dev-server": "^1.15.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/configs/jest/fileMock.js",
      "^.+\\.(css|scss)$": "<rootDir>/configs/jest/styleMock.js"
    },
    "scriptPreprocessor": "<rootDir>/configs/jest/transform.js",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "pre-commit": {
    "run": [
      "precommit:test",
      "build"
    ]
  },
  "dependencies": {
    "connect-mongo": "^1.3.2",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "express-socket.io-session": "^1.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.16.6",
    "mongoose": "^4.6.0",
    "qrcode-js": "0.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "script-loader": "^0.7.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  }
}
